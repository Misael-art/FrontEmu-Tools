{
  "system": {
    "name": "SD Emulation GUI Enterprise",
    "version": "1.0.0",
    "environment": "production",
    "platform": "windows"
  },
  "requirements": {
    "minimum_disk_space_gb": 50,
    "minimum_ram_gb": 8,
    "required_os_version": "Windows 10",
    "required_python_version": "3.8+",
    "minimum_windows_version": "10.0.19041",
    "minimum_powershell_version": 5,
    "minimum_free_space_gb": 20,
    "required_powershell_scripts": [
      "Set-ExecutionPolicy",
      "Get-WmiObject",
      "Test-Path"
    ]
  },
  "directories": {
    "config": "config",
    "logs": "logs",
    "backup": "backup",
    "reports": "reports",
    "temp": "temp"
  },
  "logging": {
    "enabled": true,
    "level": "INFO",
    "max_log_files": 10,
    "max_report_files": 5,
    "max_days_retention": 7,
    "auto_rotation": true,
    "log_format": "[%timestamp%] [%level%] %message%"
  },
  "backup": {
    "enabled": true,
    "auto_backup_before_operations": true,
    "max_backup_files": 5,
    "compression_enabled": true,
    "backup_critical_files": [
      "config.json",
      "emulator_mapping.json",
      "platform_mapping.json",
      "variant_mapping.json",
      "frontend_config.json"
    ]
  },
  "operations": {
    "timeout_seconds": 300,
    "retry_attempts": 3,
    "retry_delay_seconds": 5,
    "enable_rollback": true,
    "require_confirmation_for_critical": true
  },
  "monitoring": {
    "enabled": true,
    "check_interval_minutes": 30,
    "alert_on_errors": true,
    "performance_tracking": true,
    "disk_space_warning_threshold_gb": 10
  },
  "security": {
    "execution_policy": "Bypass",
    "validate_script_integrity": true,
    "require_admin_privileges": false,
    "log_security_events": true
  },
  "ui": {
    "show_enterprise_header": true,
    "use_colors": true,
    "show_progress_indicators": true,
    "auto_pause_after_operations": true,
    "menu_timeout_seconds": 0
  },
  "advanced": {
    "enable_experimental_features": false,
    "parallel_operations": false,
    "cache_validation_results": true,
    "optimize_for_ssd": true,
    "enable_telemetry": false
  },
  "status_codes": {
    "success": 0,
    "warning": 1,
    "error": 2,
    "critical": 3
  },
  "file_patterns": {
    "log_rotation": {
      "daily": "*.log.%Y%m%d",
      "weekly": "*.log.%Y%W",
      "monthly": "*.log.%Y%m"
    }
  },
  "organization": {
    "name": "SD Emulation GUI Enterprise",
    "department": "IT Operations",
    "contact_email": "admin@sd-emulation-gui.com",
    "support_level": "enterprise",
    "license_type": "enterprise",
    "max_concurrent_users": 100
  },
  "deployment": {
    "type": "standalone",
    "environment": "production",
    "auto_update": false,
    "update_channel": "stable",
    "rollback_enabled": true,
    "health_check_interval": 300,
    "deployment_strategy": "blue_green"
  },
  "metadata": {
    "name": "Enterprise Configuration",
    "version": "1.0.0",
    "environment": "production",
    "status": "active",
    "score": 85,
    "created_date": "2024-01-01",
    "last_modified": "2024-01-01",
    "config_version": "1.0.0",
    "author": "SD Emulation GUI Team",
    "description": "Enterprise configuration for SD Emulation GUI",
    "documentation_url": "https://docs.sd-emulation-gui.com/enterprise"
  }
}
